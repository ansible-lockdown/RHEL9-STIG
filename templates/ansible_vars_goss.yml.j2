---

## metadata for Audit benchmark
benchmark_version: {{ benchmark_version }}

## Benchmark name used by audting control role
# The audit variable found at the base
benchmark: RHEL9-STIG

timeout_ms: {{ audit_cmd_timeout }}

# turn the categories on/off
rhel9stig_cat1: {{ rhel9stig_cat1 }}
rhel9stig_cat2: {{ rhel9stig_cat2 }}
rhel9stig_cat3: {{ rhel9stig_cat3 }}

# set to redhat/rocky/alma
rhel9stig_os_distribution: {{ ansible_facts.distribution | lower }}

{% if ansible_facts.distribution == "RedHat" %}
# RHEL
rpm_gpg_key: /etc/pki/rpm-gpg/RPM-GPG-KEY-RPM-GPG-KEY-redhat-release
gpg_key:
  - name: 'release key 2'
    fingerprint: '567E 347A D004 4ADE 55BA  8A5F 199E 2F91 FD43 1D51'
  - name: 'auxiliary key'
    fingerprint: '6A6A A7C9 7C88 90AE C6AE  BFE2 F76F 66C3 D409 2792'
{% elif ansible_facts.distribution == "Rocky" %}
# rocky
rpm_gpg_key: /etc/pki/rpm-gpg/RPM-GPG-KEY-Rocky-9
gpg_key:
  - name: 'Release Engineering'
    fingerprint: '21CB 256A E16F C54C 6E65  2949 702D 426D 350D 275D'
{% elif ansible_facts.distribution == "AlmaLinux" %}
# Alma
rpm_gpg_key: /etc/pki/rpm-gpg/RPM-GPG-KEY-AlmaLinux
gpg_key:
 - name: 'AlmaLinux'
   fingerprint: '5E9B 8F56 17B5 066C E920  57C3 488F CF7C 3ABB 34F8'
{% endif %}

# If using the supplied graphical interface
rhel9stig_gui: {{ rhel9stig_gui }}

# Some tests may need to scan every filesystem or have an impact on a system
# these may need be scheduled to minimise impact
run_heavy_tests: {{ audit_run_heavy_tests }}

# tweak role to run in a non-privileged container (default value)- dynamically discovered in tasks/main.yml
system_is_container: {{ system_is_container }}

# rhel8cis/stig is left off the front of this var for consistency in testing pipeline
# system_is_ec2 toggle will disable tasks that fail on Amazon EC2 instances. Set {{ rhel_09_211010 }} to skip and false to run tasks
system_is_ec2: {{ system_is_ec2 }}

# system acts or requires router networking options
rhel9stig_system_is_router: {{ rhel9stig_system_is_router }}

